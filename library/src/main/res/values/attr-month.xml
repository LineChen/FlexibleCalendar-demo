<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--只显示一个月-->
    <declare-styleable name="MonthCalendarView">
        <attr name="startDisplayYear2" format="integer"/>
        <attr name="startDisplayMonth2" format="integer"/>
        <attr name="monthDayHorizontalSpacing2" format="dimension"/>
        <attr name="monthDayVerticalSpacing2" format="dimension"/>
        <attr name="showDatesOutsideMonth2" format="boolean"/>
        <attr name="decorateDatesOutsideMonth2" format="boolean"/>
        <attr name="disableAutoDateSelection2" format="boolean"/>
        <attr name="startDayOfTheWeek2" format="enum">
            <enum name="SUNDAY" value="1"/>
            <enum name="MONDAY" value="2"/>
            <enum name="TUESDAY" value="3"/>
            <enum name="WEDNESDAY" value="4"/>
            <enum name="THURSDAY" value="5"/>
            <enum name="FRIDAY" value="6"/>
            <enum name="SATURDAY" value="7"/>
        </attr>
    </declare-styleable>
</resources>